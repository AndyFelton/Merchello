{
	// you can define multiple editors
	"propertyEditors": [
		{
			/*this must be a unique alias*/
			"alias": "ETC.HotspotPicker",
			/*the name*/
			"name": "HotspotPicker",
			/*the icon*/
			"icon": "icon-wand",
			/*grouping for "Select editor" dialog*/
			"group": "Content Picker",
			/*the HTML file we will load for the editor*/
			"editor": {
				"view": "~/App_Plugins/Hotspots/BackOffice/Hotspots/hotspotPicker.html"
			}
		}
	],


	"gridEditors": [
			{
				"name": "HotspotPicker",
				"alias": "ETC.HotspotPicker",
				"view": "~/App_Plugins/Hotspots/GridEditors/hotspotPicker.html",
				"render": "~/App_Plugins/Hotspots/GridEditors/editor.cshtml",
				"icon": "icon-wand",
				"config": {
					"color": "red",
					"text-align": "right"
				}
			}
	],



